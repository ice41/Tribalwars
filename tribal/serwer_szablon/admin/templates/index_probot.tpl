<h2>Edytowanie graczy sterowanych przez ProBot</h2>{if !empty($error)}	<span class="error">{$error}</span><br>{/if}<form action="index.php?screen=probot&action=add_user" method="POST"/>	<table class="vis">		<tr>			<th colspan="3">Dodaj nowego gracza, który bêdzie sterowany przez bota</th>		</tr>		<tr>			<td>Nazwa: </td>			<td><input type="text" value="" name="uname"/></td>			<td><input type="submit" value="Dodaæ" name="sub"/></td>		</tr>	</table></form>{if count($BotUsers) > 0}	<h3>Lista graczy sterowanych przez bota</h3>	<table class="vis">		<tr>			<th>Nazwa</th>			<th width="25"></th>		</tr>		{foreach from=$BotUsers key=id item=user}			<tr>				<td>{$user}</td>				<td>					<a href="index.php?screen=probot&action=del_user&id={$id}"/>						<img src="/ds_graphic/delete.png" alt="usuñ"/>					</a>				</td>			</tr>		{/foreach}	</table>	{else}	<br>	<span class="error">Brak graczy kontrolowanych przez bota!</span>{/if}